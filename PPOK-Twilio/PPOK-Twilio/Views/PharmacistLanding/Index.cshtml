@model IEnumerable <PPOK.Domain.Types.Prescription>

@{
    ViewBag.Title = "Today's Notification";
}

<h1>@ViewBag.Title</h1>

<p>
    
</p>
<div>
    <input style="float:right" class="search" id="search" placeholder="Search" />
    <table class="table table-striped table-responsive">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Description
                </th>
                <th>
                    Sender Type
                </th>
                <th>
                </th>
            </tr>
        </thead>
        <tbody class="list">
            @if (Model != null)
            {
                foreach (var item in Model)
                {
                <tr>
                    <td id="name">
                        @item.Patient.LastName, @item.Patient.FirstName
                    </td>
                    <td id="descript">
                        @item.Drug.Name
                    </td>
                    <td id="senderType">
                        @item.Patient.Phone
                    </td>
                    <td>
                        <button class="btn btn-primary" data-toggle="modal" data-target="#myModalNorm">
                            Reject
                        </button>
                    </td>
                </tr>
                }
            }
        </tbody>
    </table>

    <p style="display:inline;">Last Upload Date:</p>
    <p id="demo"></p>

    <input type="file" name="pic" accept=".csv" style="display:inline;"/>
    <input type="button" value="go" style="display:inline; float:right"/>



</div>

<script src="http://code.jquery.com/jquery-1.10.1.min.js"></script>
<script src="@Url.Content("~/Scripts/csvPicker.js")"></script>