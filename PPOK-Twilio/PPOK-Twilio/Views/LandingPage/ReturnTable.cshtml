<h4 id="date"></h4>

<script>
        n = new Date();
        y = n.getFullYear();
        m = n.getMonth() + 1;
        d = n.getDate();
        document.getElementById("date").innerHTML = "Reminders for " + m + "/" + d + "/" + y + ":";
</script>

<table class="table table-striped table-responsive" id="myTable">
    <thead>
        <tr>
            <th>
                EventType
            </th>
            <th>
                Name
            </th>
            <th>
                Drug
            </th>
            <th>
                Phone
            </th>
            <th>
            </th>
        </tr>
    </thead>
    <tbody class="list">
        @if (Model != null)
            {
                foreach (var item in Model)
                {
                <tr>
                    <td id="EventType">
                        @item.EventType
                    </td>
                    <td id="name">
                        @item.LastName, @item.FirstName
                    </td>
                    <td id="descript">
                        @item.Drug
                    </td>
                    <td id="senderType">
                        @item.Phone
                    </td>
                    <td>
                        <button class="btn btn-primary" data-toggle="modal" data-target="#myModalNorm">
                            Reject
                        </button>
                    </td>
                </tr>
            }
        }
    </tbody>
</table>